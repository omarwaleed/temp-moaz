	<div style="padding: 60px 100px" id="counter" class="container text-center">
		<div class="col-sm-4"><span class="counter-value counter-styler" data-count="20">0</span><span class="counter-styler">+</span> <span class="counter-after text-capitalize">satisifed customers</span><hr></div>
		<div class="col-sm-4"><span class="counter-value counter-styler" data-count="10">0</span><span class="counter-styler">+</span> <span class="counter-after text-capitalize">services provided</span><hr></div>
		<div class="col-sm-4"><span class="counter-value counter-styler" data-count="50">0</span><span class="counter-styler">+</span> <span class="counter-after text-capitalize">projects completed</span><hr></div>
		<script>
			var a = 0;
			$(window).scroll(function() {

			var oTop = $('#counter').offset().top - window.innerHeight;
			if (a == 0 && $(window).scrollTop() > oTop) {
				$('.counter-value').each(function() {
				var $this = $(this),
					countTo = $this.attr('data-count');
				$({
					countNum: $this.text()
				}).animate({
					countNum: countTo
					},

					{

					duration: 1500,
					easing: 'swing',
					step: function() {
						$this.text(Math.floor(this.countNum));
					},
					complete: function() {
						$this.text(this.countNum);
						//alert('finished');
					}

					});
				});
				a = 1;
			}

			});
		</script>
